[{"object": "card", "id": "910959f2-db4e-4cee-9f3a-bed04d34d6f6", "oracle_id": "c556baae-77bc-4459-a925-5b94e92b336d", "multiverse_ids": [], "name": "Mini-Master // Mini-Master (cont'd)", "lang": "en", "released_at": "2022-06-10", "uri": "https://api.scryfall.com/cards/910959f2-db4e-4cee-9f3a-bed04d34d6f6", "scryfall_uri": "https://scryfall.com/card/mclb/3/mini-master-mini-master-(contd)?utm_source=api", "layout": "double_faced_token", "highres_image": true, "image_status": "highres_scan", "cmc": 0.0, "type_line": "Card // Card", "color_identity": [], "keywords": [], "card_faces": [{"object": "card_face", "name": "Mini-Master", "mana_cost": "", "type_line": "Card", "oracle_text": "2 players | 2 packs. 10 minutes | Basic Lands\nOBJECTIVE:\nPlay a quick game of Magic using only the cards in a single pack. You may also know this game as Pack Wars!\nGET READY:\nEach player opens a pack and sets aside any cards without Magic card backs, trying not to look at the rest of the cards. Then they add three of each basic land (Plains, Island, Swamp, Mountain, and Forest) to their pack and shuffle. This becomes the player's deck.\nLET'S PLAY:\nRandomly determine who goes first, then play a game of Magic! There are many variants of Mini-Master. The following are modified rules you can add to your game:", "colors": [], "image_uris": {"small": "https://cards.scryfall.io/small/front/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142", "normal": "https://cards.scryfall.io/normal/front/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142", "large": "https://cards.scryfall.io/large/front/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142", "png": "https://cards.scryfall.io/png/front/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.png?1664729142", "art_crop": "https://cards.scryfall.io/art_crop/front/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142", "border_crop": "https://cards.scryfall.io/border_crop/front/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142"}}, {"object": "card_face", "name": "Mini-Master (cont'd)", "mana_cost": "", "type_line": "Card", "oracle_text": "If you have two or more copies of any basic land, you can tap other lands as though they were lands of that type. This can help you pay costs with lots of mana symbols in them.\nAfter the first game, add another pack to your deck, look at the contents, then cut cards until you have 30. Do this again for game three!\nNot feeling your deck? Replace the whole thing with a new pack between games!\nTO WIN:\nThe first player to win two games wins the match.", "colors": [], "image_uris": {"small": "https://cards.scryfall.io/small/back/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142", "normal": "https://cards.scryfall.io/normal/back/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142", "large": "https://cards.scryfall.io/large/back/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142", "png": "https://cards.scryfall.io/png/back/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.png?1664729142", "art_crop": "https://cards.scryfall.io/art_crop/back/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142", "border_crop": "https://cards.scryfall.io/border_crop/back/9/1/910959f2-db4e-4cee-9f3a-bed04d34d6f6.jpg?1664729142"}}], "legalities": {"standard": "not_legal", "future": "not_legal", "historic": "not_legal", "timeless": "not_legal", "gladiator": "not_legal", "pioneer": "not_legal", "modern": "not_legal", "legacy": "not_legal", "pauper": "not_legal", "vintage": "not_legal", "penny": "not_legal", "commander": "not_legal", "oathbreaker": "not_legal", "standardbrawl": "not_legal", "brawl": "not_legal", "alchemy": "not_legal", "paupercommander": "not_legal", "duel": "not_legal", "oldschool": "not_legal", "premodern": "not_legal", "predh": "not_legal"}, "games": ["paper"], "reserved": false, "game_changer": false, "foil": true, "nonfoil": true, "finishes": ["nonfoil", "foil"], "oversized": false, "promo": false, "reprint": false, "variation": false, "set_id": "b5e232e5-cd5