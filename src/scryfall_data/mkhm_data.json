ge": true, "image_status": "highres_scan", "cmc": 0.0, "type_line": "Card", "color_identity": [], "keywords": [], "card_faces": [{"object": "card_face", "name": "Careful Study (minigame)", "mana_cost": "", "type_line": "Card", "oracle_text": "2-6 players | 1 pack | 5 minutes\nOBJECTIVE:\nTake turns memorizing _Magic_ cards and trying to stump your opponent with questions!\nGET READY:\nSplit up into two teams of one or more players. Randomly determine which team goes first, and then open a pack and remove any remaining land cards and any cards without _Magic_ card backs. Each team then picks a card and puts it face down on the table without looking at it.\nLET'S PLAY:\nOn your turn, your team has 30 seconds to examine your card before handing it over to the opposing team. Then they ask your team five questions about your card. Once you've answered them all, note your score. Now it's your team's turn to ask the questions! Questions can be about anything on the card.", "colors": [], "image_uris": {"small": "https://cards.scryfall.io/small/front/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781", "normal": "https://cards.scryfall.io/normal/front/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781", "large": "https://cards.scryfall.io/large/front/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781", "png": "https://cards.scryfall.io/png/front/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.png?1630031781", "art_crop": "https://cards.scryfall.io/art_crop/front/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781", "border_crop": "https://cards.scryfall.io/border_crop/front/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781"}}, {"object": "card_face", "name": "Careful Study (cont'd)", "mana_cost": "", "oracle_text": "For example:\nWhat's the creature's power?\nWhat's the mana cost?\nWho's quoted in the flavor text?\nWhat's the character in the art holding?\nIs the character's mouth open?\nWho's the artist?\nWhat's the collector number?\nWhat mechanics are mentioned in the card text?\nWhat's the rarity?\nCan you see the sun or moon in the art?\nDon't ask any questions you can't accurately answer (e.g. how many blades of grass are in the art?).\nTO WIN:\nThe team who gets the most questions right wins! If there is a tie, play another round with new cards.", "colors": [], "image_uris": {"small": "https://cards.scryfall.io/small/back/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781", "normal": "https://cards.scryfall.io/normal/back/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781", "large": "https://cards.scryfall.io/large/back/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781", "png": "https://cards.scryfall.io/png/back/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.png?1630031781", "art_crop": "https://cards.scryfall.io/art_crop/back/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781", "border_crop": "https://cards.scryfall.io/border_crop/back/6/6/6676e2f8-7625-4c0e-bfed-bed36fb8440c.jpg?1630031781"}}], "legalities": {"standard": "not_legal", "future": "not_legal", "historic": "not_legal", "timeless": "not_legal", "gladiator": "not_legal", "pioneer": "not_legal", "modern": "not_legal", "legacy": "not_legal", "pauper": "not_legal", "vintage": "not_legal", "penny": "not_legal", "commander": "not_legal", "oathbreaker": "not_legal", "standardbrawl": "not_legal", "brawl": "not_legal", "alchemy": "not_legal", "paupercommander": "not_legal", "duel": "not_legal", "oldschool": "not_legal", "premodern": "not_legal", "predh": "not_legal"}, "games": ["paper"], "reserved": false, "game_changer": false, "foil": false, "nonfoil": true, "finishes": ["nonfoil"], "oversized": false, "promo": false, "reprint": false, "variation": false, "set_id": "56782271-1c28-4bc5-8187-a7ec2e7e9be5", "set": "mkhm", "set_name": "Kaldheim Minigames", "set_type": "minigame", "set_uri": "https://api.scryfall.com/sets/56782271-1c28-4bc5-8187-a7ec2e7e9be5", "set_search_uri": "https://api.scryfall.com/cards/search?order=set&q=e%3Amkhm&unique=